<form action='/quizes' method='get'>
<input type='text' name='search'/>
<input type='submit' value='Buscar' />
</form>
<table>
<% var i; for (i=0; i< quizes.length; i++){ %>
	<tr>
	<td><a href="quizes/<%=quizes[i].id %>"><%= quizes[i].pregunta %></td>
	<td><a href="quizes/<%=quizes[i].id %>/edit"><button type="button">Editar</button></td>
	</tr>
<% } %>
</table>
<p><a href="/quizes/new"><button type="button">Crear pregunta</button></a></p>